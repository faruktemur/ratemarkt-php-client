{
  "name": "ratemarkt/ratemarkt-php-client",
  "type": "library",
  "description": "PHP client library for Ratemarkt API",
  "homepage": "http://www.ratemarkt.com",
  "keywords": [
    "ratemarkt",
    "ratemarkt.com",
    "ratemarkt php",
    "ratemarkt api",
    "ratemarkt api php client",
    "ratemarkt api php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Faruk Tmeur",
      "homepage": "https://github.com/frktmr"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors"
  }
}
